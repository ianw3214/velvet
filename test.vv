def error(theta1 : f32, theta2 : f32, input : f32, output : f32) @ f32 {
    theta1 + theta2 * input - output
}

def print_first(arr : [f32; 10]) @ i32 {
    printf(arr[1]);
    arr[1] = 40.0;
    0
}

